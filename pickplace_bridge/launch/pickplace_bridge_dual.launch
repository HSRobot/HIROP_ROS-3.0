<?xml version="1.0"?>
<launch>

  <arg name="generator_config_path0" default="$(find pickplace_bridge)/config/serialGenConfig.yaml"/>
  <arg name="actuator_config_path0"  default="$(find pickplace_bridge)/config/serialPPConfig.yaml"/>
  <arg name="move_group_name0"  default="arm0"/>
  <arg name="open_gripper_server_name0" default="/UR51/openGripper" />
  <arg name="close_gripper_server_name0" default="/UR51/closeGripper" />

  <node pkg="pickplace_bridge" type="pickplace_bridge" name="pickplace_bridge0" output="screen">
     <param name="generator_config_path" type="str" value="$(arg generator_config_path0)"/>
     <param name="actuator_config_path" type="str" value="$(arg actuator_config_path0)"/>
     <param name="move_group_name" type="str" value="$(arg move_group_name0)"/>
     <param name="open_gripper_server_name" type="str" value="$(arg open_gripper_server_name0)" />
     <param name="close_gripper_server_name" type="str" value="$(arg close_gripper_server_name0)" />
     <!-- <remap from="/pickup/result" to="" />
     <remap from="/place/result" to="" /> -->
  </node>


  <arg name="generator_config_path1" default="$(find pickplace_bridge)/config/serialGenConfig.yaml"/>
  <arg name="actuator_config_path1"  default="$(find pickplace_bridge)/config/serialPPConfig.yaml"/>
  <arg name="move_group_name1"  default="arm1"/>
  <arg name="open_gripper_server_name1" default="/UR52/openGripper" />
  <arg name="close_gripper_server_name1" default="/UR52/closeGripper" />

  <node pkg="pickplace_bridge" type="pickplace_bridge" name="pickplace_bridge1" output="screen">
     <param name="generator_config_path" type="str" value="$(arg generator_config_path1)"/>
     <param name="actuator_config_path" type="str" value="$(arg actuator_config_path1)"/>
     <param name="move_group_name" type="str" value="$(arg move_group_name1)"/>
     <param name="open_gripper_server_name" type="str" value="$(arg open_gripper_server_name1)" />
     <param name="close_gripper_server_name" type="str" value="$(arg close_gripper_server_name1)" />
     <!-- <remap from="/pickup/result" to="" />
     <remap from="/place/result" to="" /> -->
  </node>


</launch>
